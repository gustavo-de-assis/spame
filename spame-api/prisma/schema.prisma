// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  name      String
  birthdate DateTime
  gender    String
  cpf       String   @unique
  rg        String
  mother    String
  father    String
  email     String   @unique
  phone     String
  age       String
  createdAt DateTime @default(now())

  @@map("users")
}
